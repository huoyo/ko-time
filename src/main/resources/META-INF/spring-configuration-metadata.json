{
  "groups": [
    {
      "name": "ko-time",
      "type": "cn.langpy.kotime.config.DefaultConfig",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    }
  ],
  "properties": [
    {
      "name": "ko-time.context-path",
      "type": "java.lang.String",
      "defaultValue": "http://localhost:8080",
      "description": "server url for static resources",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.enable",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "to enable kotime",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.exception-enable",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "to enable exception listener",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.log-enable",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "to print log in console",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.language",
      "defaultValue": "chinese",
      "type": "java.lang.String",
      "description": "to select language for viewer",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.log-language",
      "defaultValue": "chinese",
      "type": "java.lang.String",
      "description": "to select language for log printed",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig",
      "deprecation": {
        "level": "warning",
        "replacement": "ko-time.language"
      }
    },
    {
      "name": "ko-time.pointcut",
      "type": "java.lang.String",
      "description": "pointcut for aop",
      "defaultValue": "execution(* com.example..*.*(..))",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.saver",
      "type": "java.lang.String",
      "defaultValue": "memory",
      "description": "a place to store data",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.data-source",
      "type": "java.lang.String",
      "defaultValue": "",
      "description": "datasource",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.data-prefix",
      "type": "java.lang.String",
      "defaultValue": "",
      "description": "dataPrefix",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.data-reset",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "to delete all data",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.redis-template",
      "type": "java.lang.String",
      "defaultValue": "",
      "description": "StringRedisTemplateâ€˜s bean",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.thread-num",
      "type": "java.lang.Integer",
      "defaultValue": 2,
      "description": "number of thread to store data",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.threshold",
      "type": "java.lang.Double",
      "defaultValue": 800.0,
      "description": "red will appear if avgRunTime>threshold",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.discard-rate",
      "type": "java.lang.Double",
      "defaultValue": 0.3,
      "description": "data will be discard if Math.random()<discard-rate",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.auth-enable",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "to enable authentication",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.user-name",
      "type": "java.lang.String",
      "defaultValue": "",
      "description": "userName for authentication",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.password",
      "type": "java.lang.String",
      "defaultValue": "",
      "description": "password for authentication",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    },
    {
      "name": "ko-time.param-analyse",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "the charger of analysing params",
      "sourceType": "cn.langpy.kotime.config.DefaultConfig"
    }
  ],
  "hints": []
}